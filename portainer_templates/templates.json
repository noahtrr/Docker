{
    "version": "2",
    "templates": [
        {
            "type": 3,
            "title": "RomM",
            "repository": {
                "url": "https://github.com/noahtrr/Docker",
                "stackfile": "Containers/RomM/docker-compose.yml"
            },
            "description": "Scan, enrich, and browse your game collection with a clean and responsive interface. With support for multiple platforms, various naming schemes and custom tags, RomM is a must-have for anyone who plays on emulators.",
            "name": "romm",
            "logo": "https://romm.app/_ipx/q_80/images/blocks/logos/romm-dark.svg",
            "env": [
                {
                    "name": "DB_HOST",
                    "label": "Database container hostname",
                    "description": "The docker containers hostname for the RomM database.",
                    "default": "romm-db",
                    "preset": true
                },
                {
                    "name": "DB_NAME",
                    "label": "Database Name",
                    "description": "The databases name.",
                    "default": "romm",
                    "preset": true
                },
                {
                    "name": "DB_USER",
                    "label": "Database Username",
                    "description": "The databases username.",
                    "default": "romm-user",
                    "preset": true
                },
                {
                    "name": "DB_PASSWD",
                    "label": "Database Password RomM",
                    "description": "The databases password, this entry is for the RomM configuration.",
                    "default": "romm",
                    "preset": false
                }
            ],
            "categories": ["Games", "Organizing"]
        }
    ]
}