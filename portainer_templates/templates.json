{
    "version": "2",
    "templates": [
        {
            "type": 3,
            "title": "RomM",
            "repository": {
                "url": "https://github.com/noahtrr/Docker",
                "stackfile": "Containers/RomM/docker-stack.yml"
            },
            "description": "Scan, enrich, and browse your game collection with a clean and responsive interface. With support for multiple platforms, various naming schemes and custom tags, RomM is a must-have for anyone who plays on emulators.",
            "name": "romm",
            "logo": "https://romm.app/_ipx/q_80/images/blocks/logos/romm-dark.svg",
            "env": [
                {
                    "name": "MARIADB_ROOT_PASSWORD",
                    "label": "Database root password",
                    "description": "The databases root password.",
                    "default": "db-root-password",
                    "preset": true
                },
                {
                    "name": "MARIADB_DATABASE",
                    "default": "romm-db",
                    "preset": true
                },
                {
                    "name": "MARIADB_USER",
                    "default": "romm-user",
                    "preset": true
                },
                {
                    "name": "MARIADB_PASSWORD",
                    "label": "Database password",
                    "description": "The databases password.",
                    "default": "db-password",
                    "preset": false
                },
                {
                    "name": "DB_HOST",
                    "default": "romm-db",
                    "preset": true
                },
                {
                    "name": "DB_NAME",
                    "default": "romm",
                    "preset": true
                },
                {
                    "name": "DB_USER",
                    "default": "romm-user",
                    "preset": true
                },
                {
                    "name": "DB_PASSWD",
                    "label": "Database Password RomM Entry",
                    "description": "The databases password, this entry is for the RomM configuration. (Enter the password you put for the MariaDB Password here.)",
                    "default": "db-password",
                    "preset": false
                }
            ],
            "categories": ["Games", "Organizing"]
        }
    ]
}
