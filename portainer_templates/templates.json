{
    "version": "2",
    "templates": [
        {
            "type": 3,
            "title": "RomM",
            "repository": {
                "url": "https://github.com/noahtrr/Docker",
                "stackfile": "Containers/RomM/docker-stack.yml"
            },
            "description": "Scan, enrich, and browse your game collection with a clean and responsive interface. With support for multiple platforms, various naming schemes and custom tags, RomM is a must-have for anyone who plays on emulators.",
            "name": "romm",
            "logo": "https://romm.app/_ipx/q_80/images/blocks/logos/romm-dark.svg",
            "env": [
                {
                    "name": "MARIADB_ROOT_PASSWORD",
                    "label": "Database root password",
                    "description": "The databases root password.",
                    "default": "DB_PASSWD",
                    "preset": true
                },
                {
                    "name": "MARIADB_DATABASE",
                    "default": "romm-db",
                    "preset": true
                },
                {
                    "name": "MARIADB_USER",
                    "default": "romm-user",
                    "preset": true
                },
                {
                    "name": "MARIADB_PASSWORD",
                    "label": "Database password",
                    "description": "The databases password.",
                    "default": "db-password",
                    "preset": false
                },
                {
                    "name": "DB_HOST",
                    "default": "romm-db",
                    "preset": true
                },
                {
                    "name": "DB_NAME",
                    "default": "romm",
                    "preset": true
                },
                {
                    "name": "DB_USER",
                    "default": "romm-user",
                    "preset": true
                },
                {
                    "name": "DB_PASSWD",
                    "label": "Database Password RomM Entry",
                    "description": "The databases password, this entry is for the RomM configuration. (Enter the password you put for the MariaDB Password here.)",
                    "default": "DB_PASSWD",
                    "preset": true
                },
                {
                    "name": "ROMM_AUTH_SECRET_KEY",
                    "label": "RomM Auth Secret Key",
                    "description": "Generate a key with `openssl rand -hex 32`",
                    "default": "your generated key",
                    "preset": false
                },
                {
                    "name": "IGDB_CLIENT_ID",
                    "label": "IGDB Client ID",
                    "description": "Generate an ID and SECRET in IGDB",
                    "default": "client id",
                    "preset": false
                },
                {
                    "name": "IGDB_CLIENT_SECRET",
                    "label": "IGDB Client Secret",
                    "description": "https://api-docs.igdb.com/#account-creation",
                    "default": "client secret",
                    "preset": false
                },
                {
                    "name": "MOBYGAMES_API_KEY",
                    "label": "Mobygames API Key",
                    "description": "https://www.mobygames.com/info/api/",
                    "default": "api key",
                    "preset": false
                },
                {
                    "name": "STEAMGRIDDB_API_KEY",
                    "label": "StreamgridDB API Key",
                    "description": "https://github.com/rommapp/romm/wiki/Generate-API-Keys#steamgriddb",
                    "default": "api key",
                    "preset": false
                },
                {
                    "name": "PORT",
                    "label": "Port",
                    "description": "The Port where the container should be published on the host.",
                    "default": "8080",
                    "preset": false
                },
                {
                    "name": "PORT_INTERNAL",
                    "default": "8080",
                    "preset": true
                },
                {
                    "name": "library",
                    "label": "Library Path",
                    "description": "Your game library. Check https://github.com/rommapp/romm?tab=readme-ov-file#folder-structure for more details.",
                    "default": "/path/to/library",
                    "preset": false
                },
                {
                    "name": "assets",
                    "label": "Asset Path",
                    "description": "Uploaded saves, states, etc.",
                    "default": "/path/to/assets",
                    "preset": false
                },
                {
                    "name": "config",
                    "label": "Config Path",
                    "description": "Path where config.yml is stored.",
                    "default": "/path/to/config",
                    "preset": false
                }
            ],
            "categories": ["Games", "Organizing"],
            "note": "<a href="https://github.com/rommapp/romm">link to the RomM Project</a>"
        }
    ]
}
